---
import HTMLBody, {type PropsStruct as Props} from './html.astro';
import TopBar from '../components/topbar.astro';
import FooterBar from '../components/footer.astro';

const {title, meta_description, disable_overflow} = Astro.props
---

<HTMLBody title={title} meta_description={meta_description} disable_overflow={disable_overflow}>
	<TopBar/>
	<slot/>
	<FooterBar/>
</HTMLBody>